{
  "buildCommand": "cd client && npm run build:no-check",
  "outputDirectory": "client/dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    }
  ],
  "env": {
    "VITE_API_URL": "https://orbitrum-connect-production.up.railway.app"
  }
} 
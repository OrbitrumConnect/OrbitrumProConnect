{
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "client/dist",
  "installCommand": "echo 'Installing dependencies...'",
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://orbitrum-connect-production.up.railway.app",
    "NODE_ENV": "production"
  }
} 
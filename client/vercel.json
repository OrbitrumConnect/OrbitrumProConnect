{
  "buildCommand": "npm run build:no-check",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_API_URL": "https://orbitrum-connect-production.up.railway.app",
    "NODE_ENV": "production"
  },
  "functions": {
    "src/**/*.tsx": {
      "maxDuration": 30
    }
  }
} 